pagenumber = <%= @page_number %>
if (pagenumber <= 1) {
	<% js = escape_javascript(
		render(partial: 'dreams/list', locals: { dreams: @dreams, dreams2: @dreams2, page_number: @page_number })
	) %>
	$("#filterrific_results").waitForImages(function() {
		$(this).html("<%= js %>");
	});

	<% js6 = escape_javascript(
		render(partial: 'dreams/list_count', locals: { dreams: @dreams })
	) %>
	$("#list-count").html("<%= js6 %>");
	
}

<% js2 = escape_javascript(
	render(partial: 'dreams/graph', locals: { graph_text: @graph_text })
) %>
$("#profile-graph2").html("<%= js2 %>");

<% js3 = escape_javascript(
	render(partial: 'dreams/tags', locals: { hashes: @hashes })
) %>
$("#hash-listing").html("<%= js3 %>");

<% js5 = escape_javascript(
	render(partial: 'dreams/emotions', locals: { emotions: @emotions })
) %>

$("#emotion-listing").waitForImages(function() {
	$(this).html("<%= js5 %>");
});