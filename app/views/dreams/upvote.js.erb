
<% if current_user.voted_as_when_voted_for @dream %> 
	$("#upvote_"+"<%= @dream.id %>").html("<%= escape_javascript(render('dreams/upvote')) %>")
<% else %>
	$("#upvote_"+"<%= @dream.id %>").html("<%= escape_javascript(render('dreams/no_up_vote')) %>")
<% end %>
$("#downvote_"+"<%= @dream.id %>").html("<%= escape_javascript(render('dreams/no_downvote')) %>")
$("#counter_" + "<%= @dream.id %>").html("<%= @dream.get_upvotes.size - @dream.get_downvotes.size %>")